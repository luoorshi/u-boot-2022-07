{
    "makefile.extensionOutputFolder": "./.vscode"
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true

        "**/*.cmd": true
        "**/*.tmp": true
        "**/*.dtb": true
        "**/[a-r]*.dts*": true
        "**/s[0-t]*.dts*": true
        "**/[t-z]*.dts*": true

        "*/board/[0-e]*": true
        "*/board/[g-r]*": true
        "*/board/s[0-t]*": true
        "*/board/s[v-z]*": true
        "*/board/[t-z]*": true
    }
}